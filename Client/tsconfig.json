{
   "compileOnSave": false,
   "compilerOptions": {
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
         "@models/*": [
            "src/app/core/models/*"
         ],
         "@services/*": [
            "src/app/core/services/*"
         ],
         "@shared/*": [
            "src/app/shared/*"
         ]
      },
      "declaration": false,
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "target": "es5",
      "typeRoots": [
         "node_modules/@types"
      ],
      "lib": [
         "es2017",
         "dom"
      ]
   }
}